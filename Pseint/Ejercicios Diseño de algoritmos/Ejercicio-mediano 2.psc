Proceso sin_titulo
	Escribir "Ingrese cantidad de dinero del usuario 1 ";
	Leer cant1;
	Escribir "Ingrese cantidad de dinero del usuario 2 ";
	Leer cant2;
	Escribir "Ingrese cantidad de dinero del usuario 3 ";
	Leer cant3;
	cantT<- (cant1+cant2+cant3);
	porc1<- (cant1*100)/cantT;
	porc2<- (cant2*100)/cantT;
	porc3<- (cant3*100)/cantT;
	Escribir "Las inversiones de los usuarios son: ";
	Escribir "Usuario 1: ",porc1,"%, Usuario 2: ",porc2,"%, Usuario 3: ",porc3,"%";
FinProceso
