Proceso sin_titulo
	Escribir "Ingrese 4 numeros";
	Leer num1,num2,num3,num4;
	si(num1>num2)Entonces
		max1<-num1;
	Sino
		max1<-num2;
	FinSi
	
	si(num3>num4)Entonces
		max2<-num3;
	Sino
		max2<-num4;
	FinSi
	
	si(max1>max2)Entonces
		Escribir "El numero mayor es ",max1;
	Sino
		escribir "El numero mayor es ",max2;
	FinSi
	
FinProceso
