Proceso sin_titulo
	cont<-0;
	Escribir "Ingrese la cantidad de empleados";
	Leer cantE;
	Repetir
		cont<-cont+1;
		Escribir "Empleado N°",cont;
		Escribir "Ingrese el grado al que usted pertenesca (1)(2)(3)";
		Leer grad;
		Segun grad Hacer
			1:
				sueldoB<-450000;
			2:
				sueldoB<-550000;
			3:
				sueldoB<-700000;
		Fin Segun
			Escribir "Ingrese la cantidad de horas mensuales";
			Leer horM;
			horE<-(horM-40);
			si(horE>3)Entonces
				si(horE>7)Entonces
					sueldoAc<-(sueldoB+3500*horE);
				sino
					sueldoAc<-(sueldoB+3000*horE);
				FinSi
			Sino
				sueldoAc<-(sueldoB+2000*horE);
			FinSi
			Escribir "Ingrese su ISAPRE (1)Colmena(2)Ban medica(3)Vida 3(4)Fonasa";
			Leer ISA;
			Segun ISA Hacer
				1:
					descISA<-(sueldoB*6.5)/100;
				2:
					descISA<-(sueldoB*7.3)/100;
				3:
					descISA<-(sueldoB*7)/100;
				De Otro Modo:
					descISA<-(sueldoB*7)/100;
			Fin Segun
				Escribir "Ingrese su AFP (1)BanSantander(2)Geometrica SA(3)Cuprum(4)Provida";
				Leer afp;
				Segun afp Hacer
					1:
						descAFP<-(sueldoB*12)/100;
					2:
						descAFP<-(sueldoB*13.4)/100;
					3:
						descAFP<-(sueldoB*12.3)/100;
					De Otro Modo:
						descAFP<-(sueldoB*12.9)/100;	
				Fin Segun
					descT<-(descISA+descAFP);
					sueldoT<-sueldoAc-descT;
					Escribir "Su sueldo total es ",sueldoT;
	Hasta Que (cont==cantE)
FinProceso
